\n  p.y = f * (p1.y - p0.y)\r\n}\r\n\r\nexport const getMovementVectorOrthogonalCW = (p, p0, p1, f) => {\r\n\r\n  getMovementVector(p, p0, p1, f)\r\n\r\n  const x = p.x\r\n  const y = p.y\r\n\r\n  p.x = -y\r\n  p.y = x\r\n}\r\n\r\nexport const getMovementVectorOrthogonalCCW = (p, p0, p1, f) => {\r\n\r\n  getMovementVector(p, p0, p1, f)\r\n\r\n  const x = p.x\r\n  const y = p.y\r\n\r\n  p.x = y\r\n  p.y = -x\r\n}\r\n\r\nexport const getIntersectionPoint = (p, pk0, pk1, pg0, pg1) => {\r\n\r\n  //y1 = m1 * x1  + t1 ... y2 = m2 * x2 + t1\r\n  //m1 * x  + t1 = m2 * x + t2\r\n  //m1 * x - m2 * x = (t2 - t1)\r\n  //x * (m1 - m2) = (t2 - t1)\r\n\r\n  let dx1 = (pk1.x - pk0.x); if(dx1 === 0) dx1 = 0.01;\r\n  let dy1 = (pk1.y - pk0.y); if(dy1 === 0) dy1 = 0.01;\r\n\r\n  let dx2 = (pg1.x - pg0.x); if(dx2 === 0) dx2 = 0.01;\r\n  let dy2 = (pg1.y - pg0.y); if(dy2 === 0) dy2 = 0.01;\r\n\r\n  const m1 = dy1 / dx1\r\n  const t1 = pk1.y - m1 * pk1.x\r\n\r\n  const m2 = dy2 / dx2\r\n  const t2 = pg1.y - m2 * pg1.x\r\n\r\n  let m1m2 = (m1 - m2); if(m1m2 === 0) m1m2 = 0.01;\r\n  let t2t1 = (t2 - t1); if(t2t1 === 0) t2t1 = 0.01;\r\n\r\n  const px = t2t1 / m1m2\r\n  const py = m1 * px + t1\r\n\r\n  p.x = px\r\n  p.y = py\r\n}\r\n","export const getURLParameter = (url, param) => {\r\n\r\n  if(url.includes('?')) {\r\n\r\n    const splitURL    = url.split(\"?\")\r\n    const splitParams = splitURL[1].split(\"&\")\r\n\r\n    for(let i = 0; i < splitParams.length; i++) {\r\n\r\n      const splitParam = splitParams[i].split(\"=\")\r\n\r\n      if(splitParam[0] === param) {\r\n\r\n        return splitParam[1]\r\n      }\r\n    }\r\n  }\r\n\r\n  return null\r\n}\r\n\r\nexport default { getURLParameter }\r\n","import { log, warn }          from './utils__logging.js'\r\nimport { isVideo, isCanvas }  from './utils__checks.js'\r\n\r\nconst logName = 'utils__image_data: '\r\n\r\nconst enterFrameObjects = []\r\n\r\nlet _onEnterFrame       = null\r\nlet _onExitFrame        = null\r\n\r\nconst getEFOByI